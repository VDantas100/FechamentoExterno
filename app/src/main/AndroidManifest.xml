        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".atividades.Login"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".atividades.Menuprincipal"
            android:label="@string/title_activity_menuprincipal"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".atividades.Produtos"
            android:label="@string/title_activity_produtos"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".atividades.Arquivos"
            android:label="@string/title_activity_arquivos"
            android:screenOrientation="portrait"
