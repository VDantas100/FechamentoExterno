            android:name=".atividades.TipoDocumentos"
            android:label="@string/title_activity_tipo_documentos"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".atividades.DetalheArquivo"
            android:label="@string/title_activity_detalhe_arquivo"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".atividades.VisualizarArquivo"
            android:label="@string/title_activity_visualizar_arquivo"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".atividades.Localidade"
            android:label="@string/title_activity_localidade"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".atividades.Bairro"
            android:label="@string/title_activity_bairro"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
