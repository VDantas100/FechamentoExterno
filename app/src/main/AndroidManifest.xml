            android:name=".atividades.tipodocumentosoc"
            android:label="@string/title_activity_tipodocumentosoc"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".atividades.ArquivosOC"
            android:label="@string/title_activity_arquivos_oc"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".atividades.tipodocumentosoc1"
            android:screenOrientation="portrait" />
        <activity
            android:name=".atividades.Pesquisar"
            android:screenOrientation="portrait" />
        <activity android:name=".atividades.VisualizarFile" />
    </application>

</manifest>