            android:name=".atividades.Arquivos1"
            android:label="@string/title_activity_arquivos1"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".atividades.TipoDocumentos1"
            android:label="@string/title_activity_tipo_documentos1"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
